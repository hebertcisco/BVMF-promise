{
  "name": "bvmf",
  "version": "1.0.66",
  "description": "Promise-based abstraction integrated with statusinvest.com.br for stock quotes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:cov": "npm run test -- --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/bvmf.git"
  },
  "keywords": [
    "promise",
    "typescript",
    "quotes",
    "invest",
    "stock"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/bvmf/issues"
  },
  "homepage": "https://github.com/hebertcisco/bvmf#readme",
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cheerio": "0.22.31",
    "@types/iconv-lite": "0.0.1",
    "@types/jest": "29.5.2",
    "@types/random-useragent": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.44.0",
    "eslint-plugin-jest": "27.2.2",
    "jest": "29.5.0",
    "jest-cucumber": "^3.0.1",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.2.2",
    "cheerio": "^1.0.0-rc.12",
    "iconv-lite": "^0.6.3",
    "random-useragent": "^0.5.0",
    "slugify": "^1.6.5",
    "zod": "^3.20.2"
  }
}
